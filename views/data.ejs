<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Data</title>
</head>
<body>
    <h1>Patient Data</h1>
    <table border="1">
        <thead>
            <tr>
                <th>ID</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Date of Birth</th>
            </tr>
        </thead>
        <tbody>
            <% results.forEach(patients => { %>
                <tr>
                    <td><%= patients.patient_id %></td>
                    <td><%= patients.first_name %></td>
                    <td><%= patients.last_name %></td>
                    <td><%= patients.date_of_birth %></td>

                </tr>
            <% }); %>
        </tbody>
    </table>
</body>
</html>
