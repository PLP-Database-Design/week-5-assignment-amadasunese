<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Providers Specialty</title>
</head>
<body>
    <h1>Providers Data</h1>
    <table border="1">
        <thead>
            <tr>
                <th>ID</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Provider Specialty</th>
            </tr>
        </thead>
        <tbody>
            <% results.forEach(providers => { %>
                <tr>
                    <td><%= providers.provider_id %></td>
                    <td><%= providers.first_name %></td>
                    <td><%= providers.last_name %></td>
                    <td><%= providers.provider_specialty %></td>

                </tr>
            <% }); %>
        </tbody>
    </table>
</body>
</html>
